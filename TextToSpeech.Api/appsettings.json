{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "JwtConfig": {
    "Issuer": "https://localhost:7057",
    "Audience": "https://localhost:7057",
    "ExpirationMinutes": 60,
    "Symmetric": {
      "Key": "your-very-long-secret-key"
    },
    "Asymmetric": {
      "PrivateKey": "-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----",
      "PublicKey": "-----BEGIN PUBLIC KEY-----\n...\n-----END PUBLIC KEY-----"
    }
  },

  "EmailConfig": {
    "EmailTo": "receiver@example.com",
    "EmailFrom": "sender@example.com",
    "EmailFromPassword": "aaaa bbbb cccc dddd"
  },

  "NarakeetConfig": {
    "ApiUrl": "https://api.narakeet.com",
    "ApiKey": "narakeet-api-key"
  },

  "ElasticsearchConfig": {
    "Url": "http://localhost:9200",
    "Username": "elastic",
    "Password": "elastic-password"
  },

  "ConnectionStrings": {
    "DefaultConnection": "User ID=dev;Password=db-password;Host=localhost;Port=5432;Database=tts;Pooling=true;Include Error Detail=true;",
    "Redis": "localhost:6379,allowAdmin=true"
  },

  "AppDataPath": "C:/TtsApp/Data",
  "OPENAI_API_KEY": "openai-secret-key",
  "GoogleCloudApiKey": "google-cloud-api-key"
}